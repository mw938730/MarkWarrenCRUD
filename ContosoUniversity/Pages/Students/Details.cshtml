public async Task<IActionResult>
    OnGetAsync(int? id)
    {
    if (id == null)
    {
    return NotFound();
    }

    Student = await _context.Student
    .Include(s => s.Enrollments)
    .ThenInclude(e => e.Course)
    .AsNoTracking()
    .FirstOrDefaultAsync(m => m.ID == id);

    if (Student == null)
    {
    return NotFound();
    }
    return Page();
    }
